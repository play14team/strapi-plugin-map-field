{
  "name": "strapi-plugin-map-field",
  "description": "Strapi plugin that provides a Mapbox custom field",
  "license": "MIT",
  "author": "CÃ©dric PONTET <cedric.pontet@gmail.com>",
  "version": "2.0.0",
  "keywords": [
    "strapi",
    "plugin",
    "mapbox"
  ],
  "strapi": {
    "kind": "plugin",
    "name": "map-field",
    "displayName": "Map Field",
    "description": "Strapi plugin that provides a Mapbox custom field"
  },
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^5.0.3",
    "@mapbox/mapbox-sdk": "^0.16.1",
    "@strapi/design-system": "^2.0.0-rc.27",
    "@strapi/icons": "^2.0.0-rc.27",
    "mapbox-gl": "^3.13.0",
    "react-intl": "^7.1.11",
    "react-map-gl": "^8.0.4"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.17.0",
    "@strapi/typescript-utils": "^5.17.0",
    "@types/mapbox-gl": "^3.4.1",
    "@types/mapbox__mapbox-gl-geocoder": "^5.0.0",
    "@types/mapbox__mapbox-sdk": "^0.16.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-map-gl": "^6.1.7",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  }
}